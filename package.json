{
  "name": "@ryanfw/universal-route",
  "version": "5.0.0",
  "description": "Routing system for React vertical slice architecture applications.",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryan-mahoney/Universal-Route"
  },
  "author": "Ryan Mahoney",
  "license": "MIT",
  "homepage": "https://github.com/ryan-mahoney/Universal-Route",
  "keywords": [
    "react-component",
    "react",
    "router"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "demo": "cd demo && npm run dev",
    "bench": "npm run build && node --expose-gc bench/benchmark.mjs"
  },
  "dependencies": {
    "history": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^19.2.13",
    "babel-jest": "^30.2.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "nprogress": "^0.2.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "nprogress": "^0.2.0",
    "react": "^19.1.1"
  }
}
